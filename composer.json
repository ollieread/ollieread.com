{
    "name": "ollieread/ollieread.com",
    "type": "project",
    "description": "The ollieread.com website.",
    "keywords": [
        "ollieread"
    ],
    "license": "MIT",
    "version": "1.7.2",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "ext-xmlwriter": "*",
        "doctrine/dbal": "^2.9",
        "drewm/mailchimp-api": "^2.5",
        "fideloper/proxy": "^4.0",
        "google/apiclient": "^2.0",
        "hashids/hashids": "^4.0",
        "intervention/image": "^2.5",
        "laravel/framework": "^6.0",
        "laravel/socialite": "^4.1",
        "laravel/tinker": "^1.0",
        "league/commonmark": "^1.0",
        "league/commonmark-extras": "^1.1",
        "league/fractal": "^0.19.2",
        "socialiteproviders/discord": "^2.0",
        "understand/understand-laravel5": "^2.1",
        "wildbit/swiftmailer-postmark": "^3.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "itsgoingd/clockwork": "^4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "Ollieread\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "authors": [
        {
            "name": "Ollie Read",
            "email": "me@ollieread.com"
        }
    ]
}
